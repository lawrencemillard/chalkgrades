{
    "rewrites": [
        {
          "source": "/api/(.*)",
          "destination": "/api"
        },
        {
          "source": "/(.*)",
          "destination": "/index.html"
        }
    ],
    "framework": "vite",
    "functions": {
        "memory": 1024,
        "maxDuration": 10
    }
}
